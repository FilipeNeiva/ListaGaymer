<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/playingFragment">

    <activity
        android:id="@+id/accountCreateActivity"
        android:name="com.example.listagaymer.AccountCreateActivity"
        android:label="activity_account_create"
        tools:layout="@layout/activity_account_create" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.listagaymer.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/loginActivity"
        android:name="com.example.listagaymer.LoginActivity"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />
    <fragment
        android:id="@+id/playingFragment"
        android:name="com.example.listagaymer.PlayingFragment"
        android:label="fragment_playing"
        tools:layout="@layout/fragment_playing" />
    <fragment
        android:id="@+id/doneGameFragment"
        android:name="com.example.listagaymer.DoneGameFragment"
        android:label="fragment_done_game"
        tools:layout="@layout/fragment_done_game" />
    <fragment
        android:id="@+id/intentGameFragment"
        android:name="com.example.listagaymer.IntentGameFragment"
        android:label="fragment_intent_game"
        tools:layout="@layout/fragment_intent_game" />
</navigation>